<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name = "SmokeTest" parallel="false" thread-count="3" verbose="1">
<!-- <suite name = "SmokeTest" parallel="tests" thread-count="3" verbose="1"> -->

    <parameter name="baseUrl" value="https://www.facebook.com/" />
    <parameter name="browser" value="chrome" />
    
   <test name = "TestPass">
      <classes>
         <class name = "com.avtomat.tests.TC2_TestPass" />
      </classes>
   </test>
   
   <test name = "TestFail">
      <classes>
         <class name = "com.avtomat.tests.TC1_TestFail" />
      </classes>
   </test>
   
	<test name = "TestSkip">
      <classes>
         <class name = "com.avtomat.tests.TestSkip" />
      </classes>
   </test>

   
</suite>	